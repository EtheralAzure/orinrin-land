;RETURN 2：思慕
;RETURN 3：恋慕
;RETURN 4：恋人
;==================================================
@AME36_FALL_STATE
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
IF TALENT:LOVER_AMER
	RETURNF 4
ELSEIF TALENT:AFFECTION_AMER
	RETURNF 3
ELSEIF TALENT:YEARNING_AMER
	RETURNF 2
ELSEIF CFLAG:AME36_ACQUAINTANCE
	RETURNF 1
ENDIF
RETURNF 0

