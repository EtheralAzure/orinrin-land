;==================================================
;以陷落状态需要为上限的回忆值
;==================================================
@AME36_EFFECTIVE_RECOLLECTION
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
LOCAL = AME36_RECOLLECTION()
SELECTCASE AME36_FALL_STATE()
	CASE 4
		RETURNF LOCAL
	CASE 3
		RETURNF MIN(LOCAL, AME36_RECOLLECTION4)
	CASE 2
		RETURNF MIN(LOCAL, AME36_RECOLLECTION3)
	CASE 1
		RETURNF MIN(LOCAL, AME36_RECOLLECTION2)
	CASEELSE
		RETURNF MIN(LOCAL, AME36_RECOLLECTION1)
ENDSELECT

;==================================================
;对回忆值的相守值部分计算的包装
;==================================================
@AME36_PERSISTENCE_RECOLLECTION
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
LOCAL = CFLAG:AME36_PERSISTENCE
SELECTCASE LOCAL
	CASE IS <= 40
		LOCAL = 10 * LOCAL
	CASE 41 TO 100
		LOCAL = (80 * LOCAL - 2000) / 3
	CASE 100 TO 200
		LOCAL = 30 * LOCAL - 1000
	CASEELSE
		LOCAL = (125 * LOCAL - 5000) / 4
ENDSELECT
SELECTCASE AME36_FALL_STATE()
	CASE 4
		LOCAL = MIN(LOCAL, 10000)
	CASE 3
		LOCAL = MIN(LOCAL, 5000)
	CASE 2
		LOCAL = MIN(LOCAL, 2000)
	CASE 1
		LOCAL = MIN(LOCAL, 400)
	CASE 0
		LOCAL = MIN(LOCAL, 100)
ENDSELECT
RETURNF LOCAL

;==================================================
;对回忆值的酒经历部分计算的包装
;酒共31种
;==================================================
@AME36_ALCOHOL_RECOLLECTION
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
LOCAL = AME36_BINARY_SUM(CFLAG:AME36_ALCOHOL_TYPE)
IF LOCAL <= 4
	LOCAL *= 20
ELSE
	LOCAL = 80 + 920 * (LOCAL - 4) / 31
ENDIF
SELECTCASE AME36_FALL_STATE()
	CASE 0
		RETURNF 0
	CASE 1
		RETURNF MIN(LOCAL, 200)
	CASE 2
		RETURNF MIN(LOCAL, 500)
	CASE 3, 4
		RETURNF MIN(LOCAL, 1000)
ENDSELECT

;==================================================
;对回忆值的料理经历部分计算的包装
;常规料理共174种
;==================================================
@AME36_DISH_RECOLLECTION
#FUNCTION
#LOCALSIZE 2
#LOCALSSIZE 1
LOCAL = AME36_BINARY_SUM(CFLAG:AME36_DISH_TYPE_1) + AME36_BINARY_SUM(CFLAG:AME36_DISH_TYPE_2) + AME36_BINARY_SUM(CFLAG:AME36_DISH_TYPE_3)
LOCAL = 1700 * LOCAL / 174
LOCAL:1 = AME36_BINARY_SUM(CFLAG:AME36_DISH_TYPE_C1) + AME36_BINARY_SUM(CFLAG:AME36_DISH_TYPE_C2) + AME36_BINARY_SUM(CFLAG:AME36_DISH_TYPE_C3)
IF GETBIT(CFLAG:AME36_DISH_TYPE_C1, 35)
	LOCAL += 100
	LOCAL:1 -= 1
ENDIF
LOCAL += 50 * MIN(LOCAL:1, 4)
SELECTCASE AME36_FALL_STATE()
	CASE 0
		RETURNF 0
	CASE 1
		RETURNF MIN(LOCAL, 400)
	CASE 2
		RETURNF MIN(LOCAL, 1000)
	CASE 3, 4
		RETURNF MIN(LOCAL, 2000)
ENDSELECT

;==================================================
;获取一个数字所有二进制位值的和
;==================================================
@AME36_BINARY_SUM(ARG)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
VARSET LOCAL
WHILE ARG
	LOCAL += ARG & 1
	ARG >>= 1
WEND
RETURNF LOCAL

;==================================================
;对回忆值COM部分计算的包装
;ARG为记录CFLAG的值
;ARG:1 - ARG:5 为五个陷落状态的最大值
;ARG:6 为计算参数
;==================================================
@AME36_COM_RECOLLECTION(ARG, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
SELECTCASE AME36_FALL_STATE()
	CASE 4
		ARG = MIN(ARG, ARG:5)
	CASE 3
		ARG = MIN(ARG, ARG:4)
	CASE 2
		ARG = MIN(ARG, ARG:3)
	CASE 1
		ARG = MIN(ARG, ARG:2)
	CASE 0
		ARG = MIN(ARG, ARG:1)
ENDSELECT
IF ARG:6 > 0
	RETURNF ARG * (2 * ARG:5 - ARG) / ARG:6
ELSE
	RETURNF ARG * (2 * ARG:5 - ARG) * (- ARG:6)
ENDIF

;==================================================
;测试用，使所有回忆值达到最大
;==================================================
@AME36_MAX_RECOLLECTION
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
FOR LOCAL, 1201, 1254
	CFLAG:LOCAL = 99999
NEXT
CFLAG:1204 = 0
RETURNF 1

